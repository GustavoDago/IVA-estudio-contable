*       * Snippet Number:      2                                  
*       *                                                         
*       *********************************************************
*
FUNCTION _s2t14xy59     &&  m.ACCION VALID
#REGION 1
DO CASE
CASE M.ACCION = 1
	SET ORDER TO 3
	GO BOTTOM
	SCATTER MEMVAR BLANK
	M.COD_PROV = COD_PROV + 1
	SHOW GETS ENABLE
	SHOW GET M.ACCION DISABLE
	SHOW GET M.MOVIMIENTO DISABLE
	SHOW GET M.SALIR DISABLE
	SHOW GET M.ORDEN DISABLE
	_CUROBJ = 1
CASE M.ACCION = 2
	do borrapro.spr
	show gets
CASE M.ACCION = 3
	SHOW GETS ENABLE
	SHOW GET M.ACCION DISABLE
	SHOW GET M.MOVIMIENTO DISABLE
	SHOW GET M.SALIR DISABLE
	SHOW GET M.ORDEN DISABLE
	SCATTER MEMVAR
	_CUROBJ = 1
	M.COD_prov = COD_prov
ENDCASE

