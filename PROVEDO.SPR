*       *********************************************************
*       *                                                         
*       * _S2T14XY2P           m.ACEPTAR VALID                    
*       *                                                         
*       * Function Origin:                                        
*       *                                                         
*       * From Platform:       Windows                            
*       * From Screen:         PROVEDO,     Record Number:   12   
*       * Variable:            m.ACEPTAR                          
*       * Called By:           VALID Clause                       
*       * Object Type:         Push Button                        
*       * Snippet Number:      1                                  
*       *                                                         
*       *********************************************************
*
FUNCTION _s2t14xy2p     &&  m.ACEPTAR VALID
#REGION 1
IF M.ACEPTAR = 1
	DO CASE
	CASE M.ACCION = 1
			IF .NOT. M.NOMBRE >  "    "
				WAIT WINDOW NOWAIT "ESCRIBA EL NOMBRE"
				SEN = .T.
				_CUROBJ = 2
			ENDIF
			IF M.CUIT > "         "
				SET ORDER TO 2
				SEEK M.CUIT
				IF FOUND()
					WAIT WINDOW NOWAIT "CUIT REPETIDO"
					SEN = .T.
					_CUROBJ = 3
				ENDIF
			ELSE
				WAIT WINDOW NOWAIT "ESCRIBA EL CUIT"
				SEN = .T.
					_CUROBJ = 3
			ENDIF
			IF SEN = .F.
				INSERT INTO provedor FROM MEMVAR
				SHOW GETS DISABLE
				SHOW GET M.ACCION ENABLE
				SHOW GET M.MOVIMIENTO ENABLE
				SHOW GET M.ACEPTAR DISABLE
				SHOW GET M.ORDEN ENABLE
				SHOW GET M.SALIR ENABLE
			ELSE
				SEN = .F.
			ENDIF
	CASE M.ACCION = 3
		IF M.CUIT > "  -        - "
			IF M.CUIT <> CUIT
				SET ORDER TO 2
				SEEK M.CUIT
				IF FOUND()
					WAIT WINDOW NOWAIT "CUIT REPETIDO"
					SEN = .T.
					_CUROBJ = 3
				ENDIF
			ENDIF
		ELSE
				WAIT WINDOW NOWAIT "ESCRIBA EL CUIT"
				SEN = .T.
				_CUROBJ = 3
		ENDIF
		SET ORDER TO 3
		SEEK M.COD_PROV
		IF .NOT. M.NOMBRE > "    "	
			WAIT WINDOW NOWAIT "ESCRIBA EL NOMBRE"
			SEN = .T.
			_CUROBJ = 2
		ENDIF
		IF SEN = .F.
			SET ORDER TO 3
			SEEK M.COD_prov
			GATHER MEMVAR
			SHOW GETS DISABLE
			SHOW GET M.ACCION ENABLE
			SHOW GET M.MOVIMIENTO ENABLE
			SHOW GET M.ACEPTAR DISABLE
			SHOW GET M.ORDEN ENABLE
			SHOW GET M.SALIR ENABLE
		ELSE
			SEN = .F.
		ENDIF
ENDCASE
ELSE
	SHOW GETS DISABLE
	SHOW GET M.ACCION ENABLE
	SHOW GET M.MOVIMIENTO ENABLE
	SHOW GET M.ACEPTAR DISABLE
	SHOW GET M.SALIR ENABLE
	SHOW GET M.ORDEN ENABLE
ENDIF
set order to ORDEN
SHOW GETS

